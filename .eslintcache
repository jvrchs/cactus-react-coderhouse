ency: \"CLP\"}).format(price)\r\n    } \r\n\r\n    const cartSortedByName = cart => {\r\n        const orderedCart = cart.sort((a, b) => {\r\n            const itemA = a.itemId.toLowerCase();\r\n            const itemB = b.itemId.toLowerCase();  \r\n            if (itemA < itemB) {\r\n                return -1\r\n            }\r\n            if (itemA > itemB) {\r\n                return 1\r\n            }\r\n            return 0\r\n        })\r\n     \r\n        setCart(orderedCart)\r\n    }\r\n\r\n    const addItem = (id, qty) => {\r\n\r\n        let tempArr = [];\r\n\r\n        const itemObject = {\r\n            itemId: id,\r\n            quantity: qty\r\n        }\r\n\r\n        for (let i = 0; i < cart.length; i++) {\r\n            if (cart.length !== 0) {\r\n                if (id !== cart[i].itemId) {\r\n                    tempArr.push(cart[i]); \r\n                } else {\r\n                    itemObject.quantity = qty + cart[i].quantity\r\n                } \r\n            }\r\n        }\r\n        \r\n        tempArr.push(itemObject);\r\n\r\n        cartSortedByName(tempArr);    \r\n    } \r\n\r\n    const removeItem = id => {\r\n        const tempArr = cart.filter(item => item.itemId !== id);\r\n        cartSortedByName(tempArr); \r\n    }\r\n\r\n    const updateCart = (id, newQty) => {\r\n\r\n        let tempArr = cart.filter(item => item.itemId !== id);\r\n\r\n        const itemObject = {\r\n            itemId: id,\r\n            quantity: newQty.count\r\n        };\r\n        \r\n        tempArr.push(itemObject);\r\n        \r\n        cartSortedByName(tempArr); \r\n    }\r\n\r\n    const clearCart = () => {\r\n        setCart([]);\r\n    }\r\n\r\n    const isInCart = id => {\r\n        const tempArr = cart.filter(item => item.itemId === id);\r\n        return(tempArr.length > 0 ? true : false);\r\n    }\r\n\r\n    return (\r\n        <Provider value={{cart, setCart, addItem, removeItem, clearCart, isInCart, clpCurrencyFormat, updateCart, tempCart}}>\r\n            {children}\r\n        </Provider>\r\n    )   \r\n}\r\n\r\nexport default CartContext\r\n","C:\\Users\\jchie\\Documents\\Programación\\Cursos\\Coderhouse\\React Js\\cactus-react-coderhouse\\src\\components\\context\\context.js",[],"C:\\Users\\jchie\\Documents\\Programación\\Cursos\\Coderhouse\\React Js\\cactus-react-coderhouse\\src\\components\\widgets\\CartInfo\\CartInfo.js",[],"C:\\Users\\jchie\\Documents\\Programación\\Cursos\\Coderhouse\\React Js\\cactus-react-coderhouse\\src\\components\\widgets\\CartTotal\\CartTotal.js",[],"C:\\Users\\jchie\\Documents\\Programación\\Cursos\\Coderhouse\\React Js\\cactus-react-coderhouse\\src\\components\\widgets\\ScrollToTop\\ScrollToTop.js",["233"],"import React, {useEffect} from 'react';\r\nimport {useLocation} from 'react-router-dom';\r\n\r\nconst ScrollToTop = () => {\r\n\r\n    const {pathname} = useLocation();\r\n\r\n    useEffect(() => {\r\n        window.scrollTo(0, 0);\r\n    }, [pathname])\r\n    \r\n    return null\r\n}\r\n\r\nexport default ScrollToTop\r\n","C:\\Users\\jchie\\Documents\\Programación\\Cursos\\Coderhouse\\React Js\\cactus-react-coderhouse\\src\\components\\widgets\\CounterNew\\ItemCount.js",["234","235"],"import React, { useState, useContext } from 'react';\r\nimport Counter from './Counter';\r\nimport {context} from '../../context/context'\r\nimport { FaLightbulb } from 'react-icons/fa';\r\n\r\nconst ItemCount = (props) => {\r\n\r\n    const {className, stockQty, quantity, id, tempCart, setTempCart} = props;\r\n\r\n    const {cart} = useContext(context)\r\n    console.log(tempCart);\r\n\r\n    const counterObj = {count: quantity}\r\n\r\n    const [counterValue, setCounterValue] = useState(counterObj)\r\n\r\n    const decrease = () => {\r\n        if(counterValue.count <= 1) {\r\n        }else {\r\n            setCounterValue({count: counterValue.count - 1});\r\n        }  \r\n    };\r\n\r\n    const increase = () => {\r\n        if(counterValue.count >= stockQty) {\r\n        } else 